{
  "name": "studybuddy-backend",
  "version": "1.0.0",
  "description": "Backend API server for StudyBuddy",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "npx prisma generate && tsc",
    "vercel-build": "npx prisma generate && tsc",
    "postinstall": "npx prisma generate",
    "start": "node dist/index.js",
    "seed": "tsx --require dotenv/config scripts/seed.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "demo": "tsx --require dotenv/config scripts/google-calendar-demo.ts"
  },
  "prisma": {
    "seed": "tsx --require dotenv/config scripts/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "6.7.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "googleapis": "^159.0.0",
    "openai": "^5.20.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "20.6.2",
    "jest": "^30.1.3",
    "prisma": "6.7.0",
    "ts-jest": "^29.4.1",
    "tsx": "4.20.3",
    "typescript": "5.2.2"
  }
}
